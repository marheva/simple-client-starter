{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [["@babel/plugin-proposal-class-properties"]],
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "@babel/plugin-transform-runtime"
      ]
    }
  }
}
